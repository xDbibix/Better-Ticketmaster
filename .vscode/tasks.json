{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Backend: mvnw compile",
      "type": "shell",
      "command": "${workspaceFolder}\\backend\\mvnw.cmd",
      "args": [
        "-DskipTests",
        "compile"
      ],
      "options": {
        "cwd": "${workspaceFolder}\\backend"
      },
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Backend: mvnw test",
      "type": "shell",
      "command": "${workspaceFolder}\\backend\\mvnw.cmd",
      "args": [
        "test"
      ],
      "options": {
        "cwd": "${workspaceFolder}\\backend"
      },
      "problemMatcher": []
    },
    {
      "label": "Backend: spring-boot:run (dev)",
      "type": "shell",
      "command": "${workspaceFolder}\\backend\\mvnw.cmd",
      "args": [
        "-DskipTests",
        "spring-boot:run"
      ],
      "options": {
        "cwd": "${workspaceFolder}\\backend",
        "env": {
          "SPRING_PROFILES_ACTIVE": "dev"
        }
      },
      "isBackground": true,
      "problemMatcher": []
    },
    {
      "label": "Backend: spring-boot:run (dev, foreground)",
      "type": "shell",
      "command": "${workspaceFolder}\\backend\\mvnw.cmd",
      "args": [
        "-DskipTests",
        "spring-boot:run"
      ],
      "options": {
        "cwd": "${workspaceFolder}\\backend",
        "env": {
          "SPRING_PROFILES_ACTIVE": "dev"
        }
      },
      "isBackground": false,
      "problemMatcher": []
    }
  ]
}
